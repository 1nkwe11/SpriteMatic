NODE_ENV=development
PORT=4000
FRONTEND_ORIGIN=https://spritematic.com
LOG_DIRECTORY=./logs
LOG_LEVEL=info
ENABLE_TCPDUMP=false
TCPDUMP_PATH=tcpdump
TCPDUMP_INTERFACE=
TCPDUMP_FILTER=
TCPDUMP_EXTRA_ARGS=
TCPDUMP_FILE_NAME=capture.pcap

DATABASE_URL=postgresql://postgres:postgres@localhost:5432/spritematic?schema=public
REDIS_URL=redis://localhost:6379
START_WORKER_INLINE=true

JWT_ACCESS_SECRET=replace-with-64-char-secret-access
JWT_REFRESH_SECRET=replace-with-64-char-secret-refresh
JWT_ACCESS_EXPIRES_MINUTES=15
JWT_REFRESH_EXPIRES_DAYS=7
BCRYPT_ROUNDS=12
CSRF_SECRET=replace-with-random-csrf-secret

OPENAI_API_KEY=sk-...
OPENAI_IMAGE_MODEL=gpt-image-1
WANDB_API_KEY=
OPENAI_FINE_TUNE_WANDB_PROJECT=
OPENAI_FINE_TUNE_WANDB_ENTITY=
OPENAI_FINE_TUNE_WANDB_TAGS=spritematic
STRICT_QUALITY_GATE=true
QUALITY_MIN_SCORE=82
QUALITY_MAX_ATTEMPTS=4
MAX_ESTIMATED_GENERATION_COST_USD=100

S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_REGION=us-east-1
S3_BUCKET=spritematic
S3_ENDPOINT=http://localhost:9000
S3_PUBLIC_BASE_URL=http://localhost:9000/spritematic

DAILY_DEFAULT_LIMIT=50
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100
AUTH_RATE_LIMIT_MAX=10
